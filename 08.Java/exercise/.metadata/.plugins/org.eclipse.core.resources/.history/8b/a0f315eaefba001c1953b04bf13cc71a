package ex_06;

public class Employee {
	String id;
	String name;
	int money;
	
	Employee() {
	}
	Employee(String id, String name, int money) {
		this.id = id;
		this.name = name;
		this.money = money;
	}
	
	public void bonus(int bonus){
		return money+bonus;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}
	
}
