package ex_06;

public class Ex06_teacherTest_10 {
	public static void main(String[] args) {
		/* 홍길동이 통장을 개설했다 */
		JBank hong = new JBank("홍길동", 12500);
		
		/* 이순신이도 통장을 개설했다. */
		JBank lee= new JBank("이순신", 59000);
		
		hong.setMoney(30000);
		lee.setMoney(70000);
		
		System.out.println("%s씨 통장에 금액 %d원을 다시 넣었습니다.%n", hong.getName());
		
		/* 홍길동씨 통장에서 금액을 30000 다시 넣었습니다. */
		/* 이순신씨 통장에서 금액을 70000 다시 넣었습니다. */
		/* setter 재저장 */
		
		/* 홍길동씨 통장 금액은 30000 입니다. */
		/* 이순신씨 통장 금액은 70000 입니다. */
	}
}
