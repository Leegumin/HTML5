package 기초연습문제_선생님.DTO.예제_02;

public class DTOTest {

	public static void main(String[] args) {
		BoardDTO board = new BoardDTO();
		board.setIdx(0);
		common.setDeleteYn(true);
		common.setInsertTime("등록일");
		common.setUpdateTime("수정일");
		common.setDeleteTime("2022-04-26 10:48");

		common.info();

		AttachDTO comment = new AttachDTO();
		comment.setIdx(1);
		comment.setBoardIdx(1);
		comment.setContent("게시판 내용");
		comment.setWriter("글쓴이");

		comment.info();
	}

}
