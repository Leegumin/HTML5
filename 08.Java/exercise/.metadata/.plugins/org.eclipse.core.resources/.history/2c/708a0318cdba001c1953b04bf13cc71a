package ex_06;

public class Student {
	String name;
	int hakbun;
	int kor;
	int eng;
	int math;

	Student() {
	}

	Student(String name, int hakbun, int kor, int eng, int math) {
		name= name;
		hakbun = hakbun;
		this.kor = kor;
		eng = eng;
		math = math;
	}

	int sum() {
		int result = 0;
		result = kor+eng+math;
		return result;
	}
}
