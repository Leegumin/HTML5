package 기초연습문제_선생님.CommonDTO;

public class CommentDTO extends CommonDTO {
	private int idx; // 글번호
	private int boardIdx; // 게시판id
	private String content; // 게시판내용
	private String writer; // 글쓴이

	public void info() {
		System.out.println(getPaginationInfo());
		System.out.println(getDeleteYn());
		System.out.println(getInsertTime());
		System.out.println(getUpdateTime());
		System.out.println(getDeleteTime());
		System.out.println(getIdx());
		System.out.println(getBoardIdx());
		System.out.println(getContent());
		System.out.println(getWriter());
	}
	
	public int getIdx() {
		return idx;
	}

	public void setIdx(int idx) {
		this.idx = idx;
	}

	public int getBoardIdx() {
		return boardIdx;
	}

	public void setBoardIdx(int boardIdx) {
		this.boardIdx = boardIdx;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getWriter() {
		return writer;
	}

	public void setWriter(String writer) {
		this.writer = writer;
	}

}
