package 기초연습문제_선생님.DTO.예제_03_상속;

public class DTOTest {

	public static void main(String[] args) {
		System.out.println("== BoardDTO ==");
		
		BoardDTO board = new BoardDTO();
		
		board.setIdx(1);
		board.setTitle("애린왕자");
		board.setContent("오후 4시에 니가 온다카믄 내는 3시부터 행복할끼라");
		board.setWriter("프린스영");
		board.setViewCnt(5);
		board.setNoticeYn(false);
		board.setSecretYn(false);
		board.setChangeYn(false);
		board.setFileIdxs(0);

		board.info();
		
		System.out.println("== AttachDTO ==");
		
		AttachDTO attach = new AttachDTO();
		
		attach.setIdx(4);
		attach.setBoardIdx(2);
		attach.setOriginalName("어린왕자");
		attach.setSaveName("애린왕자");
		attach.setSize(300);
		
		attach.info();
	}

}
