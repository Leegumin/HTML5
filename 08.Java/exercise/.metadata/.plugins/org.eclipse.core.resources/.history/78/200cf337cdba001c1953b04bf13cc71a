package ex_06;

public class Student {
	String name;
	int hakbun;
	int kor;
	int eng;
	int math;

	Student() {
	}

	Student(String name, int hakbun, int kor, int eng, int math) {
		this.name= name;
		this.hakbun = hakbun;
		this.kor = kor;
		this.eng = eng;
		this.math = math;
	}

	int sum() {
		int result = 0;
		result = kor+eng+math;
		return result;
	}
}
