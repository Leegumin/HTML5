package ex_06;

public class Ex06_teacherTest_06 {
	public static void main(String[] args) {
		Student student1 = new Student("일준", 1, 100, 85, 90);
		Student student2 = new Student("이준", 2, 95, 80, 95);
		
		System.out.println("%d학번인 %s의 총점은%d" + student1.sum());
		System.out.println("student2 총점은" + student2.sum());
		
	}
}
